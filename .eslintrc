{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "extends": [
        "eslint:recommended", "google"
    ],
    "rules": {
        "arrow-parens": "off",
        "comma-dangle": "off",
        "semi": "off",
        "indent": "off",
        "quotes": "off",
        "eol-last": "off",
        "no-trailing-spaces": "off",
        "max-len": [2, {"code": 120, "tabWidth": 4, "ignoreUrls": true}],
        "padded-blocks": "off",
        "require-jsdoc": "off"
    }
}